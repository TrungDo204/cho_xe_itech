        $(document).ready(function () {
                    });
        function showLoginForm() {
            openModal('#modal-login');
            window.livewire.emit('showLoginForm');
        }
        function closeModal(element, callback='') {
            $(element).removeClass('is-active').trigger('modal:close', [callback]);
            $('html').removeClass('is-clipped');
        };
        function count_time() {
            timer = setTimeout(function() {
                remain_time--;
                if (remain_time <= 0) {
                    $('#btn-resend').html('Gửi lại mã');
                    // $('#btn-resend').prop('disabled', false);
                } else {
                    $('#btn-resend').html('Gửi lại mã ('+remain_time+')');
                    count_time();
                }
            }, 1000);
        }
        window.addEventListener('register-completed', event => {
            closeModal('#modal-login');
            setTimeout(() => {
                window.location.reload();
            }, 2000);
        });
        window.addEventListener('form-loaded', event => {
            $('.toggle-password').unbind('click');
            $('.toggle-password').click(function() {
                $(this).find('i.icon-eye').toggleClass('icon-eye-slash');
                var input = $($(this).closest('.field').find('input'));
                if (input.attr('type') == 'password') {
                    input.attr('type', 'text');
                } else {
                    input.attr('type', 'password');
                }
            });
        });
        window.addEventListener('init-otp-form', event => {
            if (typeof event.detail.remain_time != 'undefined') {
                remain_time = event.detail.remain_time;
                count_time();
            }
            $('.input-code').unbind('keydown');
            $('.input-code').unbind('keyup');
            $('.input-code').keydown(function (e) { 
                var v = $(this).val();
                if(v != '') {
                    $(this).val('');
                }
            });

            $('.input-code').keyup(function (e) { 
                // e.preventDefault();
                var v = $(this).val();
                if (isNaN(v)) {
                    console.log('tesst');
                    $(this).val('');
                } else {
                    console.log('tesst2');
                    var i = parseInt($(this).data('index'));
                    if (i < 6) {
                        i++;
                        $('#input-code-'+i).focus();
                        $('#input-code-'+i).select();
                    }
                }
            });
        });
    </script>
