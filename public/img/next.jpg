    <script type="text/javascript">
        function pushKeywordToList(keyword) {
            var item_element = '<div class="keyword_item"><a href="#" class="keyword">'+keyword+'</a><a href="#" class="remove"><i class="icon-remove"></i></a></div>';
            var list_element = $('.list_keywords');
            list_element.append(item_element);
        }

        document.addEventListener('DOMContentLoaded',function(e){
            var history_keywords = getCookie('history_keyword');
            if (history_keywords != '') {
                history_keywords = JSON.parse(history_keywords);
                history_keywords.forEach(function(item){
                    pushKeywordToList(item)
                });
                $('#empty_history').addClass('is-hidden');
                $('#keyword_history_wrapper').removeClass('is-hidden');
            }

            $(document).on('click', '.keyword_item .remove', function(e){
                e.preventDefault();
                var keyword = $(this).parent().text();
                removeHistoryKeyword(keyword);
                $(this).closest('.keyword_item').remove();
                if ($('.list_keywords .keyword_item').length == 0) {
                    $('#empty_history').removeClass('is-hidden');
                    $('#keyword_history_wrapper').addClass('is-hidden');
                }
            });

            $(document).on('click', '#btn_clear_history', function(e){
                e.preventDefault();
                clearHistoryKeyword();
                $('#empty_history').removeClass('is-hidden');
                $('#keyword_history_wrapper').addClass('is-hidden');
            });

            $(document).on('click', '.keyword_item .keyword', function(e){
                e.preventDefault();
                var keyword = $(this).parent().text();
                $('[name=keyword]').val(keyword);
                $('#header_search_form').submit();
            });

            $('#header_search_form').on('submit', function() {
                var keyword = $('[name=keyword]').val();
                if (keyword != '') {
                    pushHistoryKeyword(keyword);
                }
            });

            $('.clear_input_keyword').on('click', function(e) {
                e.preventDefault();
                $('#header_search_form [name=keyword]').val('');
            });
        });
    </script>
