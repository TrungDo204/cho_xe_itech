                    $('#main_search_form [name=to_year]').val(max_value);
                }
                $('#main_search_form .year_range_amount span').eq(1).text(max_value);
            };
            setYearRangeDefault();

            [
                {'select_element':'#select_condition', 'data_name':'condition', 'input_name':'condition'},
                {'select_element':'#select_carorigin', 'data_name':'carorigin', 'input_name':'carorigin'},
            ].forEach(function(item){
                $(item.select_element+' .radio_button').on('click', function(e){
                    e.preventDefault();
                    var data_value = $(this).data(item.data_name);
                    $(item.select_element+' .radio_button').not(this).removeClass('is-selected');
                    $(this).toggleClass('is-selected');
                    var selected_item = $(item.select_element+' .radio_button.is-selected').first();
                    if (selected_item.length > 0) {
                        $('#main_search_form [name='+item.input_name+']').attr('disabled', false);
                        $('#main_search_form [name='+item.input_name+']').val(data_value);
                    } else {
                        $('#main_search_form [name='+item.input_name+']').attr('disabled', true);
                    }
                });
            });

            [
                {'select_element':'#select_gear', 'data_name':'gear', 'input_name':'gear'},
                {'select_element':'#select_fuel', 'data_name':'fuel', 'input_name':'fuel'},
                {'select_element':'#select_cartype', 'data_name':'cartype', 'input_name':'cartype'},
                {'select_element':'#select_seats', 'data_name':'seats', 'input_name':'seats'},
                {'select_element':'#select_transmission', 'data_name':'transmission', 'input_name':'transmission'},
            ].forEach(function(item){
                $(item.select_element+' .radio_button').on('click', function(e){
                    e.preventDefault();
                    $(this).toggleClass('is-selected');
                    var data_value = $(this).data(item.data_name);
                    var current_value = $('#main_search_form [name='+item.input_name+']').val();
                    arr_current_value = current_value != '' ? current_value.split(',') : [];
                    if ($(this).hasClass('is-selected')) {
                        arr_current_value.push(data_value);
                    } else {
                        arr_current_value.removeItem(data_value);
                    }
                    $('#main_search_form [name='+item.input_name+']').val(arr_current_value);
                    var selected_item = $(item.select_element+' .radio_button.is-selected').first();
                    $('#main_search_form [name='+item.input_name+']').attr('disabled', !selected_item.length);
                });
            });

            $('#main_search_form').on('submit', function(e){
                ['model'].forEach(function(item){
                    if ($('#main_search_form [name='+item+']').val() == 0) {
                        $('#main_search_form [name='+item+']').attr('disabled', true);
                    }
                });
                if ($('#main_search_form [name=keyword]').val() == '') {
                    $('#main_search_form [name=keyword]').attr('disabled', true);
                }
                return true;
            });

            $('.btn_reset').on('click', function(e){
                e.preventDefault();
                $('#main_search_form [name=keyword]').val('');
                ['model'].forEach(function(item){
                    $('#main_search_form [name='+item+']').val(0);
                });
                $('#select_brand .option_selected').text('Tất cả')
                $('#select_province .option_selected').text('Toàn quốc')
                $('#select_brand .list_option > *, #select_province .list_option > *').removeClass('is-selected');
                $('#select_brand .list_option > *:first-child, #select_province .list_option > *:first-child').addClass('is-selected');
                $('#select_brand .list_option > li.is-selected, #select_province .list_option > li.is-selected').trigger('click');
                $('#main_search_form .price_slider_range').slider('values', [0, 2000000000]);
                setPriceRangeDefault();
                ['condition', 'province', 'brand', 'gear', 'fuel', 'cartype', 'seats', 'carorigin', 'transmission', 'min_price', 'max_price', 'from_year', 'to_year'].forEach(function(item){
                    $('#main_search_form [name='+item+']').attr('disabled', true);
                });
                $('.radio_button').removeClass('is-selected');
                $('#main_search_form .year_slider_range').slider('values', [2000, 2023]).trigger('slide');
                setYearRangeDefault();
            });
        });
    </script>
        <script type="text/javascript">
            window.addEventListener('toast-msg', event => {
                var Toast = Swal.mixin({
                  toast: true,
                  position: 'top',
                  showConfirmButton: false,
                  timer: 3000
                });
                if(typeof event.detail.type == 'undefined') {
                    var type = 'success';
                } else {
                    var type = event.detail.type;
                }
                Toast.fire({
                    icon: type,
                    title: event.detail.text
                  })
                // console.log(event.detail.text);
            });
            window.addEventListener('swal-msg', event => {
                Swal.fire(event.detail);
                  // title: 'Error!',
                  // text: 'Do you want to continue',
